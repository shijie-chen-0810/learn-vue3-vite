{
  "name": "learn-vue3",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev": "cross-env MOCK=none vite",
    "dev:mock": "cross-env MOCK=ok vite"
  },
  "dependencies": {
    "@vueuse/core": "^7.7.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "umi-request": "^1.4.0",
    "vue": "^3.2.25",
    "vue-request": "^1.2.4",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "cross-env": "^7.0.3",
    "mockjs": "^1.1.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.1",
    "vite": "^2.8.0",
    "vite-auto-import-resolvers": "^2.1.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-pages": "^0.21.4",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vue-tsc": "^0.29.8"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@vueuse/core",
        "nprogress",
        "pinia",
        "umi-request",
        "vue",
        "vue-request",
        "vue-router"
      ]
    }
  }
}
